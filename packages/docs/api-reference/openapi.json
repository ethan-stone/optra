{
  "openapi": "3.1.0",
  "info": { "title": "FastAPI", "version": "0.1.0" },
  "paths": {
    "/v1/internal.createRootClient": {
      "post": {
        "summary": "Create Root Client",
        "operationId": "create_root_client_v1_internal_createRootClient_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RootClientCreateReqBody"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ClientCreateResult" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        },
        "security": [{ "OAuth2ClientCredentialsBearer": [] }]
      }
    },
    "/v1/internal.createWorkspace": {
      "post": {
        "summary": "Create Workspace",
        "operationId": "create_workspace_v1_internal_createWorkspace_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/WorkspaceCreateParams" }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WorkspaceCreateResult"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        },
        "security": [{ "OAuth2ClientCredentialsBearer": [] }]
      }
    },
    "/v1/apis.createApi": {
      "post": {
        "summary": "Create Api",
        "operationId": "create_api_v1_apis_createApi_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/ApiCreateReqBody" }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ApiCreateResult" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        },
        "security": [{ "OAuth2ClientCredentialsBearer": [] }]
      }
    },
    "/v1/clients.createClient": {
      "post": {
        "summary": "Create Basic Client",
        "operationId": "create_basic_client_v1_clients_createClient_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BasicClientCreateReqBody"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ClientCreateResult" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        },
        "security": [{ "OAuth2ClientCredentialsBearer": [] }]
      }
    },
    "/v1/tokens.verifyToken": {
      "post": {
        "summary": "Verify Token",
        "operationId": "verify_token_v1_tokens_verifyToken_post",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BasicAuthorizerResult"
                }
              }
            }
          }
        },
        "security": [{ "OAuth2ClientCredentialsBearer": [] }]
      }
    },
    "/oauth/token": {
      "post": {
        "summary": "Oauth Token",
        "description": "Generate an access token for a client. This supports the client credentials flow\nwith the client_id, client_secret, and grant_type parameters being sent in a combination of\nthe request body, authorization header, and form data.",
        "operationId": "oauth_token_oauth_token_post",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/TokenResponse" }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ApiCreateReqBody": {
        "properties": {
          "name": { "type": "string", "title": "Name" },
          "scopes": {
            "anyOf": [
              {
                "items": {
                  "$ref": "#/components/schemas/ApiScopeCreateParams"
                },
                "type": "array"
              },
              { "type": "null" }
            ],
            "title": "Scopes"
          }
        },
        "type": "object",
        "required": ["name"],
        "title": "ApiCreateReqBody"
      },
      "ApiCreateResult": {
        "properties": {
          "id": { "type": "string", "title": "Id" },
          "name": { "type": "string", "title": "Name" },
          "workspace_id": { "type": "string", "title": "Workspace Id" },
          "scopes": {
            "anyOf": [
              {
                "items": { "$ref": "#/components/schemas/ApiScope" },
                "type": "array"
              },
              { "type": "null" }
            ],
            "title": "Scopes"
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time",
            "title": "Updated At"
          }
        },
        "type": "object",
        "required": ["id", "name", "workspace_id", "created_at", "updated_at"],
        "title": "ApiCreateResult"
      },
      "ApiScope": {
        "properties": {
          "id": { "type": "string", "title": "Id" },
          "name": { "type": "string", "title": "Name" },
          "description": { "type": "string", "title": "Description" },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At"
          }
        },
        "type": "object",
        "required": ["id", "name", "description", "created_at"],
        "title": "ApiScope"
      },
      "ApiScopeCreateParams": {
        "properties": {
          "name": { "type": "string", "title": "Name" },
          "description": { "type": "string", "title": "Description" }
        },
        "type": "object",
        "required": ["name", "description"],
        "title": "ApiScopeCreateParams"
      },
      "BasicAuthorizerResult": {
        "properties": {
          "valid": { "type": "boolean", "title": "Valid" },
          "reason": {
            "anyOf": [
              { "$ref": "#/components/schemas/InvalidReasons" },
              { "type": "null" }
            ]
          }
        },
        "type": "object",
        "required": ["valid"],
        "title": "BasicAuthorizerResult"
      },
      "BasicClientCreateReqBody": {
        "properties": {
          "name": { "type": "string", "title": "Name" },
          "api_id": { "type": "string", "title": "Api Id" },
          "rate_limit_bucket_size": {
            "anyOf": [{ "type": "integer" }, { "type": "null" }],
            "title": "Rate Limit Bucket Size"
          },
          "rate_limit_refill_amount": {
            "anyOf": [{ "type": "integer" }, { "type": "null" }],
            "title": "Rate Limit Refill Amount"
          },
          "rate_limit_refill_interval": {
            "anyOf": [{ "type": "integer" }, { "type": "null" }],
            "title": "Rate Limit Refill Interval"
          }
        },
        "type": "object",
        "required": ["name", "api_id"],
        "title": "BasicClientCreateReqBody"
      },
      "ClientCreateResult": {
        "properties": {
          "id": { "type": "string", "title": "Id" },
          "name": { "type": "string", "title": "Name" },
          "version": { "type": "integer", "title": "Version" },
          "workspace_id": { "type": "string", "title": "Workspace Id" },
          "for_workspace_id": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "For Workspace Id"
          },
          "api_id": { "type": "string", "title": "Api Id" },
          "rate_limit_bucket_size": {
            "anyOf": [{ "type": "integer" }, { "type": "null" }],
            "title": "Rate Limit Bucket Size"
          },
          "rate_limit_refill_amount": {
            "anyOf": [{ "type": "integer" }, { "type": "null" }],
            "title": "Rate Limit Refill Amount"
          },
          "rate_limit_refill_interval": {
            "anyOf": [{ "type": "integer" }, { "type": "null" }],
            "title": "Rate Limit Refill Interval"
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At"
          },
          "secret": { "type": "string", "title": "Secret" }
        },
        "type": "object",
        "required": [
          "id",
          "name",
          "version",
          "workspace_id",
          "api_id",
          "created_at",
          "secret"
        ],
        "title": "ClientCreateResult"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": { "$ref": "#/components/schemas/ValidationError" },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "InvalidReasons": {
        "type": "string",
        "enum": [
          "EXPIRED",
          "INVALID_SIGNATURE",
          "BAD_JWT",
          "NOT_FOUND",
          "RATE_LIMIT_EXCEEDED",
          "VERSION_MISMATCH",
          "SECRET_EXPIRED"
        ],
        "title": "InvalidReasons"
      },
      "RootClientCreateReqBody": {
        "properties": {
          "for_workspace_id": { "type": "string", "title": "For Workspace Id" },
          "name": { "type": "string", "title": "Name" }
        },
        "type": "object",
        "required": ["for_workspace_id", "name"],
        "title": "RootClientCreateReqBody"
      },
      "TokenResponse": {
        "properties": {
          "access_token": { "type": "string", "title": "Access Token" },
          "token_type": { "type": "string", "title": "Token Type" },
          "expires_in": { "type": "integer", "title": "Expires In" },
          "scope": {
            "anyOf": [{ "type": "string" }, { "type": "null" }],
            "title": "Scope"
          }
        },
        "type": "object",
        "required": ["access_token", "token_type", "expires_in", "scope"],
        "title": "TokenResponse"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": { "anyOf": [{ "type": "string" }, { "type": "integer" }] },
            "type": "array",
            "title": "Location"
          },
          "msg": { "type": "string", "title": "Message" },
          "type": { "type": "string", "title": "Error Type" }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      },
      "WorkspaceCreateParams": {
        "properties": { "name": { "type": "string", "title": "Name" } },
        "type": "object",
        "required": ["name"],
        "title": "WorkspaceCreateParams"
      },
      "WorkspaceCreateResult": {
        "properties": {
          "id": { "type": "string", "title": "Id" },
          "name": { "type": "string", "title": "Name" },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time",
            "title": "Updated At"
          }
        },
        "type": "object",
        "required": ["id", "name", "created_at", "updated_at"],
        "title": "WorkspaceCreateResult"
      }
    },
    "securitySchemes": {
      "OAuth2ClientCredentialsBearer": {
        "type": "oauth2",
        "flows": {
          "clientCredentials": { "scopes": {}, "tokenUrl": "oauth/token" }
        }
      }
    }
  }
}
